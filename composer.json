{
  "name": "headsnet/deploy-notify-bundle",
  "description": "Send notifications for Symfony application deployment",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Headsnet\\DeployNotifyBundle\\": "src/Headsnet/DeployNotifyBundle"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Ben Roberts",
      "email": "ben@headsnet.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0",
    "michelf/php-markdown": "^1.7",
    "psr/log": "^1.0",
    "symfony/console": "^3.3 || ~4.0",
    "symfony/framework-bundle": "^3.0 || ~4.0",
    "symfony/swiftmailer-bundle": "^2.3 || ^3.0",
    "twig/twig": "^2.0"
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  }
}
